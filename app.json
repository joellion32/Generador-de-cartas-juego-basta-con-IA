{
  "expo": {
    "name": "Basta Cartas",
    "slug": "basta-card-ia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-android.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FC1933"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.basta-card-ia"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "ios": {
        "icon": "./assets/icon-ios.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joellion32.bastacardia"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FC1933",
          "image": "./assets/splash-icon.png",
          "imageWidth": "100%",
          "imageHeight": "100%"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "37936c5a-6787-4c75-8368-6aebcb7cc264"
      }
    }
  }
}
